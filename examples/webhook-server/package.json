{
  "name": "aura-webhook-server-example",
  "version": "1.0.0",
  "description": "Production-ready webhook server for receiving Aura verification notifications",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "db:migrate": "node dist/services/database.js migrate",
    "test": "vitest",
    "test:webhook": "tsx scripts/test-webhook.ts",
    "generate:secret": "tsx scripts/generate-secret.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@aura-network/verifier-sdk": "^1.0.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "zod": "^3.22.4",
    "better-sqlite3": "^9.2.2",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "aura",
    "webhook",
    "verification",
    "blockchain",
    "credentials"
  ],
  "author": "Aura Network",
  "license": "MIT"
}
